<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <Nullable>annotations</Nullable>
    <IsMgmtLibrary>true</IsMgmtLibrary>
    <VersionPrefix>1.0.0</VersionPrefix>
    <VersionSuffix>preview.3</VersionSuffix>
    <Version>1.0.0-preview.3</Version>
  </PropertyGroup>

  <PropertyGroup>
    <LangVersion>9.0</LangVersion>
    <IncludeGeneratorSharedCode>true</IncludeGeneratorSharedCode>
    <RestoreAdditionalProjectSources>https://azuresdkartifacts.blob.core.windows.net/azure-sdk-tools/index.json</RestoreAdditionalProjectSources>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.ResourceManager.Core" VersionOverride="1.0.0-alpha.20210721.3" />
  </ItemGroup>

</Project>
