<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>1.0.0-beta.1</Version>
    <PackageId>Azure.ResourceManager.Resources</PackageId>
    <Description>
      Azure management client SDK for Azure resource provider Resources.
      This is the first beta preview version, so expect incompatible changes in subsequent releases as we improve the product.
    </Description>
    <PackageTags>azure;management;resources</PackageTags>
    <NoWarn>$(NoWarn);AZC0008;</NoWarn>
    <IncludeGeneratorSharedCode>true</IncludeGeneratorSharedCode>
    <IsMgmtSubLibrary>false</IsMgmtSubLibrary>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.ResourceManager" VersionOverride="1.0.0-alpha.20210824.1" />
    <PackageReference Include="Azure.Core" VersionOverride="1.18.0" />
  </ItemGroup>
</Project>
